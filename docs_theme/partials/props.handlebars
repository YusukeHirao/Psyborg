<article id="property_{{name}}" class="property item{{#if access}} {{access}}{{/if}}{{#if deprecated}} deprecated{{/if}}{{#if extended_from}} inherited{{/if}}">
	<header>
		<h1 class="name"><code>{{name}}</code></h1>
		{{#if since}}<span class="since">{{since}}</span>{{/if}}
		<span class="property">プロパティ</span>
		<div class="attr">
			<h2><code>{{name}}</code></h2>
			<code class="type">{{#crossLink type}}{{/crossLink}}</code>
			{{#if deprecated}}<span class="deprecated"{{#if deprecationMessage}} title="{{deprecationMessage}}"{{/if}}>deprecated</span>{{/if}}
			{{#if access}}<span class="{{access}}">{{access}}</span>{{/if}}
			{{#if final}}<span class="final">final</span>{{/if}}
			{{#if static}}<span class="static">static</span>{{/if}}
			{{#if overwritten_from}}<span class="overridden">overridden</span>{{/if}}
		</div>
		<div class="meta">
			{{#if overwritten_from}}
				<p>継承元
				<a href="{{crossLinkRaw overwritten_from/class}}#property_{{overwritten_from/name}}">{{overwritten_from/class}}</a>
				{{#if foundAt}}but overwritten in{{/if}}
			{{else}}
				{{#if extended_from}}
					<p>継承元
					<a href="{{crossLinkRaw extended_from}}#property_{{name}}">{{extended_from}}</a>:
				{{else}}
					{{#providedBy}}<p>Provided by the <a href="../modules/{{.}}.html">{{.}}</a> module.</p>{{/providedBy}}
					<p>{{#if foundAt}}定義元{{/if}}
				{{/if}}
			{{/if}}
			{{#if foundAt}}
			<a href="{{foundAt}}">`{{{file}}}:{{{line}}}`</a>
			{{/if}}
			</p>
			{{#if default}}<p><strong>Default:</strong> {{default}}</p>{{/if}}

			{{#if deprecationMessage}}
				<p>Deprecated: {{deprecationMessage}}</p>
			{{/if}}
		</div>
	</header>
	<section class="description">
		{{{propertyDescription}}}
	</section>
	{{#example}}
	<section class="example">
		<h1>例)</h1>

		<div class="example-content">
			{{{.}}}
		</div>
	</section>
	{{/example}}
